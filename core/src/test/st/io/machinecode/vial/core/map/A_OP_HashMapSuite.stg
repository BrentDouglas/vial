                .named("O$I$HashMap<Long>[" + name + "]")
                .withFeatures(
                        MapFeature.GENERAL_PURPOSE,
                        MapFeature.ALLOWS_NULL_KEYS,
                        MapFeature.ALLOWS_ANY_NULL_QUERIES,
                        CollectionFeature.SUPPORTS_ITERATOR_REMOVE,
                        CollectionFeature.SERIALIZABLE,
                        CollectionSize.ANY)
                .createTestSuite();
    }

    private static Test stringTestsForO$I$HashMap(final String name, final CreateMap create) {
        return MapTestSuiteBuilder
                .using(new O$I$MapGenerator() {
                    @Override
                    protected Map<String, $P$> create(final Map.Entry<String,$P$>[] entries) {
